{
  "name": "app-web",
  "version": "1.0.0",
  "private": true,
  "description": "app web app",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "e2e": "ng e2e",
    "build:ssr-prod": "npm run build:client-and-server-bundles-prod && npm run webpack:server",
    "build:ssr-qa": "npm run build:client-and-server-bundles-qa && npm run webpack:server",
    "build:ssr-uat": "npm run build:client-and-server-bundles-uat && npm run webpack:server",
    "build:ssr-dev": "npm run build:client-and-server-bundles-dev && npm run webpack:server",
    "build:ssr-dev-id": "npm run build:client-and-server-bundles-dev-id && npm run webpack:server",
    "serve:ssr": "node dist/server.js",
    "build:client-and-server-bundles-prod": "ng build --prod --configuration=production && ng run app-web:server -c production && node compress.js",
    "build:client-and-server-bundles-qa": "ng build --prod --configuration=qa && ng run app-web:server  && node compress.js",
    "build:client-and-server-bundles-uat": "ng build --prod --configuration=uat && ng run app-web:server -c uat && node compress.js",
    "build:client-and-server-bundles-dev": "ng build --prod --configuration=dev  && ng run app-web:server",
    "build:client-and-server-bundles-dev-id": "ng build --prod --configuration=dev-id  && ng run app-web:server",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "bundle-report": "ng build --prod --stats-json && webpack-bundle-analyzer dist/browser/stats.json"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
