    "@angular/forms": "6.1.8",
    "@angular/http": "6.1.8",
    "@angular/platform-browser": "6.1.8",
    "@angular/platform-browser-dynamic": "6.1.8",
    "@angular/pwa": "^0.8.9",
    "@angular/router": "6.1.8",
    "@angular/service-worker": "6.1.8",
    "@angular/upgrade": "6.1.8",
